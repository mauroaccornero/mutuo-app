{
    "clearMocks": true,
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": ["./src/**"],
    "coverageThreshold": {
        "global": {
            "lines": 80
        }
    },
    "moduleNameMapper": {
        "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
}
