{
    "homepage": "https://mauroaccornero.github.io/mutuo-app/",
    "name": "mortgage-app",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "webpack serve --open --config webpack.dev.js",
        "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack.prod.js",
        "lint": "eslint . && stylelint \"**/*.scss\"",
        "lint:fix": "eslint . --fix && stylelint \"**/*.scss\" --fix",
        "format": "prettier --write .",
        "test": "jest",
        "check-syntax": "tsc --noEmit",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "audit": "npm audit --production",
        "prepare": "husky install"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.8.5",
        "moment": "^2.29.4",
        "proptypes": "^1.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.19.0",
        "@babel/preset-env": "^7.19.0",
        "@babel/preset-typescript": "^7.18.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.0.1",
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "gh-pages": "^4.0.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss-loader": "^7.0.1",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sass": "^1.54.9",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "stylelint": "^14.11.0",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-standard-scss": "^5.0.0",
        "terser-webpack-plugin": "^5.3.6",
        "ts-jest": "^29.0.0",
        "ts-loader": "^9.3.1",
        "typescript": "^4.8.3",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.0",
        "webpack-merge": "^5.8.0"
    }
}
